                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT   empresa.nome
		                            , empresa.endereco
		                            , empresa.cnpj
		                            , funcionario.nome
		                            , funcionario.dscodigo
		                            , departamento.descricao
		                            , marcacao.entrada_1
		                            , marcacao.saida_1
		                            , marcacao.entrada_2
