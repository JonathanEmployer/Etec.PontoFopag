                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        funcionario, dscodigo, idocorrencia, ocorrencia, departamento, empresa, cnpj_cpf, inicioabono, fimabono, dtmarcacao, dia, abonado, parcial, abonototalmin, abonoparcialmin, dbo.FN_CONVMIN(abonoparcialmin) 
                         AS abonoParcial, dbo.FN_CONVMIN(abonototalmin) AS abonoTotal
FROM            (SELECT        f.nome AS funcionario, f.dscodigo, a.idocorrencia, o.descricao AS ocorrencia, d.descricao AS departamento, e.nome AS empresa, CASE WHEN ISNULL(e.cnpj, '') 
                                                    &lt;&gt; '' THEN e.cnpj ELSE e.cpf END AS cnpj_cpf, a.datai AS inicioabono, a.dataf AS fimabono, m.data AS dtmarcacao, m.dia, a.abonado, a.parcial, 
                                                    CASE WHEN a.parcial = 0 THEN dbo.FN_CONVHORA(m.horastrabalhadas) + dbo.FN_CONVHORA(m.horastrabalhadasnoturnas) ELSE '0' END AS abonototalmin, 
                                                    CASE WHEN a.parcial = 1 THEN dbo.FN_CONVHORA(a.horai) + dbo.FN_CONVHORA(a.horaf) ELSE '0' END AS abonoparcialmin
                          FROM            afastamento AS a INNER JOIN
                                                    marcacao_view AS m ON m.data BETWEEN a.datai AND a.dataf LEFT OUTER JOIN
                                                    funcionario AS f ON f.id = m.idfuncionario LEFT OUTER JOIN
                                                    departamento AS d ON f.iddepartamento = d.id LEFT OUTER JOIN
                                                    empresa AS e ON e.id = f.idempresa LEFT OUTER JOIN
                                                    ocorrencia AS o ON o.id = a.idocorrencia
                          WHERE        (a.tipo = 0) AND (a.idfuncionario = f.id) AND (a.abonado = 1) AND (a.datai &gt;= @datai) AND (m.data &lt;= @dataf) OR
                                                    (a.tipo = 1) AND (a.abonado = 1) AND (a.datai &gt;= @datai) AND (m.data &lt;= @dataf) AND (a.iddepartamento = f.iddepartamento) OR
                                                    (a.tipo = 2) AND (a.abonado = 1) AND (a.datai &gt;= @datai) AND (m.data &lt;= @dataf) AND (a.idempresa = f.idempresa)) AS D_1</CommandText>
                    <Parameters>
