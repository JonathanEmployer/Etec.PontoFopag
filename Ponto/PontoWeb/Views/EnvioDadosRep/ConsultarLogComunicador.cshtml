@model IList<Modelo.Proxy.PxyGridLogComunicador>
@{
    ViewBag.Title = "Consultar Log Comunicador";
}
@using (Html.BeginForm())
{
    <div class="divCenterMedio">
        <div class="panel panel-default">
            <div class="panel-heading salvando">
                @Html.ActionLink("X", "Index", "Home", null, new { @class = "close" })
                <h3 class="panel-title titulo-pagina">@ViewBag.Title</h3>
            </div>
            <div class="panel-body panelConteudo" id="panelBodyMaximo">
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="col-md-4" style="padding-left:0px;">
                                <div class="panel panel-default">
                                    <div class="panel-heading" style="padding-top:1px; padding-bottom:0px">
                                        @Html.Label("Data", new { @class = "control-label label-sm" })
                                    </div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-md-12">
                                                @Html.LabelFor(model => model.FirstOrDefault().DataImportacaoStr, new { @class = "control-label label-sm" })
                                                @Html.TextBoxFor(model => model.FirstOrDefault().DataImportacaoStr, new { @class = "form-control input-sm" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-8" style="padding-right:0px;">
                                <div class="panel panel-default">
                                    <div class="panel-heading" style="padding-top:1px; padding-bottom:0">
                                        @Html.Label("Geração do Arquivo", new { @class = "control-label label-sm" })
                                    </div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-md-12">
                                                @Html.LabelFor(model => model.FirstOrDefault().ArquivoImportado, new { @class = "control-label label-sm" })
                                                @Html.TextBoxFor(model => model.FirstOrDefault().ArquivoImportado, new { @class = "form-control input-sm" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12" style="padding-right:15px;" id="pnlLogNum">
                            <div class="panel panel-default">
                                <div class="panel-heading" style="padding-top:2px; padding-bottom:0">
                                    @Html.Label("Processamento do Arquivo", new { @class = "control-label label-sm" })
                                </div>
                                <div class="panel-body" style="padding-top:5px; padding-bottom:5px">
                                    <div class="row">
                                        <div class="col-md-2">
                                            @Html.LabelFor(model => model.FirstOrDefault().Lidos, new { @class = "control-label label-sm" })
                                            @Html.TextBoxFor(model => model.FirstOrDefault().Lidos, new { @class = "form-control input-sm" })
                                        </div>
                                        <div class="col-md-2">
                                            @Html.LabelFor(model => model.FirstOrDefault().Processados, new { @class = "control-label label-sm" })
                                            @Html.TextBoxFor(model => model.FirstOrDefault().Processados, new { @class = "form-control input-sm" })
                                        </div>
                                        <div class="col-md-2">
                                            @Html.LabelFor(model => model.FirstOrDefault().Errados, new { @class = "control-label label-sm" })
                                            @Html.TextBoxFor(model => model.FirstOrDefault().Errados, new { @class = "form-control input-sm" })
                                        </div>
                                        <div class="col-md-2">
                                            @Html.LabelFor(model => model.FirstOrDefault().Repetidos, new { @class = "control-label label-sm" })
                                            @Html.TextBoxFor(model => model.FirstOrDefault().Repetidos, new { @class = "form-control input-sm" })
                                        </div>
                                        <div class="col-md-2">
                                            @Html.LabelFor(model => model.FirstOrDefault().PontoFechado, new { @class = "control-label label-sm" })
                                            @Html.TextBoxFor(model => model.FirstOrDefault().PontoFechado, new { @class = "form-control input-sm" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="panel panel-default">
                                <div class="panel-heading" style="padding-top:1px; padding-bottom:0">
                                    @Html.Label("Marcações", new { @class = "control-label label-sm" })
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-md-5">
                                            @Html.LabelFor(model => model.FirstOrDefault().MarcacoesProcessadas, new { @class = "control-label label-sm" })
                                            @Html.TextBoxFor(model => model.FirstOrDefault().MarcacoesProcessadas, new { @class = "form-control input-sm" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                @if (Model.FirstOrDefault().ResultImport.Length > 0)
                {
                    <div class="row">
                        <div class="col-md-12">
                            @Html.LabelFor(model => model.FirstOrDefault().ResultImport, new { @class = "control-label label-sm" })
                            @Html.TextAreaFor(model => model.FirstOrDefault().ResultImport, new { @class = "form-control  input-sm" })
                        </div>
                    </div>
                }
             </div>
         </div>
            <div class="panel-footer">
                <div class="row">
                    <div class="col-md-12">
                        <div style="text-align:right">
                            <a class="btn btn-danger" href="@Url.Action("GridComunicador", "EnvioDadosRep")">
                                <span class="glyphicon glyphicon-remove salvando"></span> Cancelar
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@if (ViewBag.Consultar == 1)
{
    <script>
        $(document).ready(function () {
            $('form').find('input, textarea, button, select').attr('disabled', 'disabled');
        });
    </script>
}