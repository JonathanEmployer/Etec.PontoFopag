@model IEnumerable<Modelo.TipoVinculo>
@using System.Web.Optimization

@{
    Layout = "~/Views/Shared/_LayoutConsulta.cshtml";
}
<table id="tbSel">
    <thead>
        <tr>
            <th>Código</th>
            <th>Descrição</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr id="@item.Codigo">
                <td>@item.Codigo</td>
                <td>@item.Descricao</td>
            </tr>
        }
</table>
