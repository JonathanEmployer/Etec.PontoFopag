@model IEnumerable<Modelo.Empresa>
@using System.Web.Optimization

@{
    ViewBag.Title = "Consultar Empresa";
    Layout = "~/Views/Shared/_LayoutConsulta.cshtml";
}
<table id="tbSel" class="table table-striped table-bordered table-hover table-condensed nowrap" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Código</th>
            <th>Nome</th>
            <th>Insc. Rg.</th>
        </tr>
        <tr>
            <th class="text">Codigo</th>
            <th class="text">Nome</th>
            <th class="text">Insc. Rg.</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr id="@item.Codigo">
                <td>@item.Codigo</td>
                <td>@item.Nome</td>
                <td>@item.Cnpj</td>
            </tr>
        }
</table>
