@model Modelo.HorarioDinamicoLimiteDdsr

@Html.Partial("_Scripts")


<div class="limiteDDSR">
    <div class="col-md-11 linhaDDSR" style="padding:0px;">
            @Html.HiddenFor(x => x.Id)
            @Html.HiddenFor(x => x.IdHorarioDinamico)
            @Html.HiddenFor(x => x.Incdata)
            @Html.HiddenFor(x => x.Inchora)
            @Html.HiddenFor(x => x.Incusuario)
            @Html.HiddenFor(x => x.Acao)
            @Html.HiddenFor(x => x.Altdata)
            @Html.HiddenFor(x => x.Althora)
            @Html.HiddenFor(x => x.Altusuario)
            @Html.HiddenFor(x => x.Codigo)
            @Html.HiddenFor(x => x.Delete, new { @class = "mark-for-delete" })
            <div class="col-md-6">
                <div class="input-group input-group-sm">
                    <span class="input-group-addon">Limite DDSR</span>
                    @Html.TextBoxFor(model => model.LimitePerdaDsr, new { @class = "form-control timeL", @placeholder = "--:--" })
                    @Html.ValidationMessageFor(model => model.LimitePerdaDsr)
                    <span class="input-group-addon">Horas</span>
                </div>
            </div>
            <div class="col-md-6">
                <div class="input-group input-group-sm">
                    <span class="input-group-addon">Qtd. DDSR</span>
                    @Html.TextBoxFor(model => model.QtdHorasDsr, new { @class = "form-control timeL", @placeholder = "--:--" })
                    @Html.ValidationMessageFor(model => model.QtdHorasDsr)
                    <span class="input-group-addon">Horas</span>
                </div>

            </div>
    </div>
    <div class="col-md-1" style="padding:0px;">
        <div class="input-group input-group-sm" style="align-content:center; align-items:center;"></div>
        @Html.LinkToRemoveNestedForm("", "div.limiteDDSR", "input.mark-for-delete")
    </div>

</div>