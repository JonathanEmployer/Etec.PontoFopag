<script>
    $(document).ready(function () {
        $(":input[type='text']:enabled:visible:first").focus();
        //Habilita e Desabilita Campos dos parametros
        validaSelecao();
        //Seta eventos dos botoes
        cwk_DataCalendario('#btnDataInicial', '#DataInicial');
        cwk_DataCalendario('#btnDataFinal', '#DataFinal');
        cwk_EventoConsulta("#btnEmpresa", "#lkpEmpresa", "EventoConsulta", "Empresa", "");
        cwk_EventoConsulta("#btnDepartamento", "#lkpDepartamento", "EventoConsulta", "Departamento", "");
        cwk_EventoConsulta("#btnFuncao", "#lkpFuncao", "EventoConsulta", "Funcao", "");
        cwk_EventoConsulta("#btnFuncionario", "#lkpFuncionario", "EventoConsulta", "Funcionario", "");

        //Tratamento do Radio do tipo do lancamento
        if ($('#idRadioEmpresa').is(':checked')) {
            divVisivel("#divEmpresa");
        }
        if ($('#idRadioDepartamento').is(':checked')) {
            divVisivel("#divDepartamento");
        };

        if ($('#idRadioFuncao').is(':checked')) {
            divVisivel("#divFuncao");
        };

        if ($('#idRadioFuncionario').is(':checked')) {
            divVisivel("#divFuncionario");
        }; 
        //Quando for um cadastro novo marca esse parametros como default.
        var dataI = $('#DataInicial').val(); var dataF = $('#DataFinal').val();
        if (dataI == "" && dataF == "") {
            $('#lkpContabilizarFaltas').prop('checked', true);
            $('#lkpContAtrasosSaidasAntec').prop('checked', true);
            $('#lkpContabilizarCreditos').prop('checked',true);
        }
       
        FechamentoPercentualHE();

        VerificaLimiteConjunto();
        ////quando o campo estiver readonly joga o foco para o próximo campo
        //$("input[type='text'][readonly]").on("focus", function () {
        //    if ($(this).attr("readonly")) {
        //        var inputs = $(this).closest('form').find(':input');
        //        inputs.eq(inputs.index(this) + 1).focus();
        //    }
        //});
    });

    //Limpa erro adicionada pelo modalstate dos lkps quando receberem o foco
    $(".lkps").focusin(function () {
        limpaErro(this.name);
    });

    $("#idRadioEmpresa").change(function () {
        divVisivel("#divEmpresa");
    });
    $("#idRadioDepartamento").change(function () {
        divVisivel("#divDepartamento");
    });
    $("#idRadioFuncao").change(function () {
        divVisivel("#divFuncao");
    });
    $("#idRadioFuncionario").change(function () {
        divVisivel("#divFuncionario");
    });

    function divVisivel(idDiv) {
        $(".divsTipo").hide();
        $(idDiv).show();
    }

    // Marca/Desmarca dias do parametro
    $("#btnMarcarDias").click(function (event) {
        event.preventDefault();
        if ($(".pdias").size() == $(".pdias:checked").size()) {
            $(".pdias").prop("checked", false);
        }
        else {
            $(".pdias").prop("checked", true);
        }
        validaSelecao(event);
    });

    // Quando alterar o valor de algum checkbox revalida a tela (Habilitando/Desabilitando campos)
    $(".checkboxbh").click(function (event) {
        validaSelecao(event);
    });

    // Habilita/Desabilita campos de acordo com os parametros de checkbox
    function validaSelecao(event) {
        var selecao = "";
        if (event) {
            selecao = event.target.id;
        }

        // Se marcar Hora Extra desabilita Banco de Horas
        if (selecao == "PercentualComoHoraExtra" && $("#PercentualComoHoraExtra").is(':checked')) {
            $("#BancoHorasPorPercentual").prop("checked", false);
        }

        // Se marcar Banco de Horas desabilita Hora Extra
        if (selecao == "BancoHorasPorPercentual" && $("#BancoHorasPorPercentual").is(':checked')) {
                $("#PercentualComoHoraExtra").prop("checked", false);
        }

        VerificaBancoHorasAcumulativo();

        // Se marcado Hora Extra desabilita e limpa os campos referente a limites do Banco de Horas
        if (selecao != "ExtraPrimeiroBool" && $("#BancoprimeiroBool").is(':checked')) {
            $(".lbancohora").prop('readonly', false);
            $(".ldbancohora").prop('readonly', false);
            $(".lmbancohora").prop('readonly', false);
            $(".lsemanabancohora").prop('readonly', false);
            $(".lsBancoHora").prop('readonly', false);
            $(".lHorasExtras").prop('readonly', true);
            $(".lHorasExtras").val("--:--");
            $("#ExtraPrimeiroBool").prop("checked", false);
            $(".ldbancohora").each(function () {
                if (!$(this).val()) {
                    $(this).val("--:--");
                }
            });
            $(".lsemanabancohora").each(function () {
                if (!$(this).val()) {
                    $(this).val("--:--");
                }
            });
            $(".lmbancohora").each(function () {
                if (!$(this).val()) {
                    $(this).val("--:--");
                }
            });
            $(".lbancohora").each(function () {
                if (!$(this).val()) {
                    $(this).val("--:--");
                }
            });
            $(".lsBancoHora").each(function () {
                if (!$(this).val()) {
                    $(this).val("---:--");
                }
            });
            
        }
        else {
            $(".lbancohora").prop('readonly', true);
            $(".lsemanabancohora").prop('readonly', true);
            $(".lmbancohora").prop('readonly', true);
            $(".ldbancohora").prop('readonly', true);
            $(".lsBancoHora").prop('readonly', true);
            $(".lbancohora").val("--:--");
            $(".ldbancohora").val("--:--");
            $(".lsemanabancohora").val("--:--");
            $(".lmbancohora").val("--:--");
            $(".lsBancoHora").val("--:--");
        }

        // Se marcado Banco de Horas desabilita e limpa os campos referente a limites de Hora Extra
        if (selecao != "BancoprimeiroBool" && $("#ExtraPrimeiroBool").is(':checked')) {
            $(".lHorasExtras").prop('readonly', false);
            $(".lbancohora").prop('readonly', true);
            $(".lbancohora").val("--:--");
            $(".ldbancohora").prop('readonly', true);
            $(".ldbancohora").val("--:--");
            $(".lsemanabancohora").prop('readonly', true);
            $(".lsemanabancohora").val("--:--");
            $(".lmbancohora").prop('readonly', true);
            $(".lmbancohora").val("--:--");
            $(".lsBancoHora").prop('readonly', true);
            $(".lsBancoHora").val("--:--");
            $("#BancoprimeiroBool").prop("checked", false);
        }
        else {
            $(".lHorasExtras").prop('readonly', true);
            $(".lHorasExtras").val("--:--");
        }

        // Se banco de horas acumulativo estiver selecionado reabilita campos.
        if ($("#BancoHorasAcumulativo").is(':checked')) {
            $(".lbhsemana").prop('readonly', true);
            $(".lbhperc.lbhsemana").val(0);
            $("#liPnlTipoAcumulo").show();
        }

        VerificaLimiteConjunto();

        //Se selecionado  algum dia Valida a linha referente a semana;
        $(".pdias:checked").each(function () {
            var vdia = "." + this.id;
            $(vdia).prop('readonly', false);
        });

        //Se dia não estiver checado desabilita os campos da linha do dia
        $(".pdias:unchecked").each(function () {
            var vdia = "." + this.id;
            $(vdia).prop('readonly', true);
            $(vdia).val("--:--");
            $(vdia + ".lbhperc").val(0);
        });

        // Independente do que aconteceu acima, se não esta selecionado banco de horas por percentual não habilita o campo Percentual
        ValidaBancoDeHorasPorPercentual();
    }

    function ValidaBancoDeHorasPorPercentual() {
        if ($("#BancoHorasPorPercentual").is(':unchecked') && $("#PercentualComoHoraExtra").is(':unchecked')) {
            $(".lbhperc").val(0);
            $(".lbhperc").prop('readonly', true);
        }
        else {
            $(".pdias:unchecked").each(function () {
                var vdia = "." + this.id;
                $(vdia + ".lbhperc").val(0);
                $(vdia + ".lbhperc").prop('readonly', true);
            });
        }
    }

    // Quado selecionado algum limite na aba de acumulo dispara a funcao de validacao
    $(".plimites").click(function (event) {
        validaSelecaoAcumulo(event);
    });

    //exibe a tab referente a Banco de horas acumulativo de acordo com o checkbox .
    $("#BancoHorasAcumulativo").click(function (event) {
        if ($("#BancoHorasAcumulativo").is(':checked')) {
            $('#bhTab a:last').tab('show')
        }
        else {
            $('#bhTab a:first').tab('show')
        }
    });



    //Validação dos campos da aba de acumulo.
    function validaSelecaoAcumulo(event) {
        $(".plimites:checked").each(function () {
            var aplim = ".ap" + this.id;
            $(aplim).prop('readonly', false);
            var ahlim = ".ah" + this.id;
            $(ahlim).prop('readonly', false);
        });

        $(".plimites:unchecked").each(function () {
            var aplim = ".ap" + this.id;
            $(aplim).prop('readonly', true);
            $(aplim).val(0);
            var ahlim = ".ah" + this.id;
            $(ahlim).prop('readonly', true);
            $(ahlim).val("---:--");
        });
    }

    function VerificaLimiteConjunto() {
        if ($("#BancoHorasDiarioMensal").is(':checked')) {
            $(".pnlLimiteConjunto").show();
            $("#BancoHorasAcumulativo").prop("checked", false);
            $("#BancoHorasAcumulativo").prop('readonly', true);
        }
        else {
            $("#BancoHorasAcumulativo").prop('readonly', true);
            $(".pnlLimiteConjunto").hide();
        }
        VerificaBancoHorasAcumulativo();
    }

    function VerificaBancoHorasAcumulativo() {
        //Selecionado Banco de Horas Acumulativo desabilita os parametros de Banco de Horas e Horas extras da aba parametros
        if ($("#BancoHorasAcumulativo").is(':unchecked')) {
            $("#liPnlTipoAcumulo").hide();
            $("#ExtraPrimeiroBool").prop('readonly', false);
            ValidaBancoDeHorasPorPercentual();
        } //Se desmarcado desabilita os campos e deixa a div invisivel do acumulo
        else {
            $("#ExtraPrimeiroBool").prop('readonly', true);
            $("#ExtraPrimeiroBool").prop("checked", false);
            $("#BancoprimeiroBool").prop("checked", true);
        }
    }

    // Se informar valor em algum campo da coluna limite do Banco de horas limpa da coluna Diario referente
    $(".lbancohora").blur(function () {
        if (!$(this).attr("readonly") && ($(this).val() != "" && $(this).val() != "--:--")) {
            var limpa = this.id.substr(this.id.length - 1, 1);
            limpaLimiteHoras(limpa, "#LimiteHorasDiarios");
            limpaLimiteHoras(limpa, "#LimiteHorasSemanal");
            limpaLimiteHoras(limpa, "#LimiteHorasMensal");
        }
        if ($(this).attr("readonly")) {
            $(this).val("--:--");
        }
    });

    $(".ldbancohora").blur(function () {
        if (!$(this).attr("readonly") && ($(this).val() != "" && $(this).val() != "--:--")) {
            var limpa = this.id.substr(this.id.length - 1, 1);
            limpaLimiteHoras(limpa, "#LimiteHoras");
            limpaLimiteHoras(limpa, "#LimiteHorasSemanal");
            limpaLimiteHoras(limpa, "#LimiteHorasMensal");
        }
        if ($(this).attr("readonly")) {
            $(this).val("--:--");
        }
    });

    // Se informar valor em algum campo da coluna Saldo do Banco de horas limpa da coluna Mensal da linha referente
    $(".lsBancoHora").blur(function () {
        if (!$(this).attr("readonly") && ($(this).val() != "" && $(this).val() != "--:--")) {
            var limpa = this.id.substr(this.id.length - 1, 1);
            limpaLimiteHoras(limpa, "#LimiteHoras");
            limpaLimiteHoras(limpa, "#LimiteHorasDiarios");
            limpaLimiteHoras(limpa, "#LimiteHorasMensal");
            limpaLimiteHoras(limpa, "#LimiteHorasSemanal");
        }
        if ($(this).attr("readonly")) {
            $(this).val("--:--");
        }
    });

    $(".lsemanabancohora").blur(function () {
        if (!$(this).attr("readonly") && ($(this).val() != "" && $(this).val() != "--:--")) {
            var limpa = this.id.substr(this.id.length - 1, 1);
            limpaLimiteHoras(limpa, "#LimiteHoras");
            limpaLimiteHoras(limpa, "#LimiteHorasDiarios");
            limpaLimiteHoras(limpa, "#LimiteHorasMensal");
            limpaLimiteHoras(limpa, "#SaldoBH");
        }
        if ($(this).attr("readonly")) {
            $(this).val("--:--");
        }
    });

    // Se informar valor em algum campo da coluna Diario do Banco de horas limpa da coluna Limite da linha referente
    $(".lmbancohora").blur(function () {
        if (!$(this).attr("readonly") && ($(this).val() != "" && $(this).val() != "--:--")) {
            var limpa = this.id.substr(this.id.length - 1, 1);
            limpaLimiteHoras(limpa, "#LimiteHoras");
            limpaLimiteHoras(limpa, "#LimiteHorasDiarios");
            limpaLimiteHoras(limpa, "#LimiteHorasSemanal");
            limpaLimiteHoras(limpa, "#SaldoBH");
        }
        if ($(this).attr("readonly")) {
            $(this).val("--:--");
        }
    });

    function limpaLimiteHoras(idComponente, componente) {
        var limpa = componente + "_" + idComponente;
        $(limpa).val("--:--");
    }


    function FechamentoPercentualHE() {
        if ($("#FechamentoPercentualHE").is(':unchecked')) {
            $("#pnlFechamentoPercentualHE").hide();
        } //Se desmarcado esconde os campos 
        else {
            $("#pnlFechamentoPercentualHE").show();
        }
    }

    //Selecionado Fechamento por Percentual HE, habilita os campos de limite e percentual de fechamento
    $("#FechamentoPercentualHE").click(function (event) {
        FechamentoPercentualHE();
    });
    
</script>
