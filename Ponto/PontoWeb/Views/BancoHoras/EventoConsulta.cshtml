@model IEnumerable<Modelo.BancoHoras>
@using System.Web.Optimization

@{
    Layout = "~/Views/Shared/_LayoutConsulta.cshtml";
}
<table id="tbSel" class="table table-striped table-bordered table-hover table-condensed nowrap" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Código</th>
            <th>Início</th>
            <th>Fim</th>
            <th>Tipo do Banco</th>
            <th>Nome</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr id="@item.Id">
                <td>@item.Codigo</td>
                <td>@Html.Encode(String.Format("{0:dd/MM/yyyy}", item.DataInicial))</td>
                <td>@Html.Encode(String.Format("{0:dd/MM/yyyy}", item.DataFinal))</td>
                <td>@item.TipoDescricao</td>
                <td>@item.Nome</td>
            </tr>
        }
    </tbody>
</table>
