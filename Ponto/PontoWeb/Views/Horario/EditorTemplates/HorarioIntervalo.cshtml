                    @Html.LabelFor(model => model.Entrada_2, new { @class = "control-label label-sm" })
                </div>
            </div>
            <div class="col-md-3 col-xs-5" style="padding-left:0px; padding-right:0px">
                <div class="col-md-4 col-xs-4">
                    @Html.LabelFor(model => model.Saida_2, new { @class = "control-label label-sm" })
                </div>
                <div class="col-md-2 col-xs-1">
                </div>
                <div class="col-md-4 col-xs-4">
                    @Html.LabelFor(model => model.Entrada_3, new { @class = "control-label label-sm" })
                </div>
            </div>
            <div class="col-md-offset-0 col-md-3 col-xs-offset-2 col-xs-5" style="padding-left:0px; padding-right:0px">
