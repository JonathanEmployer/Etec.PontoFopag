                </div>
            </div>
            <div class="col-md-1 col-xs-4" style="padding-left:0px; padding-right:0px">
                <div class="col-md-12 col-xs-12">
                    @Html.LabelFor(model => model.Saida_4, new { @class = "control-label label-sm" })
                </div>
            </div>
        </div>
    </div>
}
<div class="row">
    <div class="col-md-1 col-xs-12">@Html.Label(Model.DiaDesc, new { @class = "control-label label-sm" })</div>
    <div class="col-md-11 col-xs-12">
        <div class="col-md-1 col-xs-2" style="padding-left:0px; padding-right:0px">
            <div class="form-group">
                @Html.TextBoxFor(model => model.Entrada_1, new { @class = "form-control input-sm timeL EntradaIntervalo", @Style = "background:#CBE5E7;", @placeholder = "--:--", id = string.Format("txtEntrada1{0}", Model.Dia), @readonly="readonly" })
                @Html.ValidationMessageFor(model => model.Entrada_1)
            </div>
        </div>
        <div class="col-md-3 col-xs-5">
            <div class="row">
                <div class="col-md-4 col-xs-4">
                    <div class="form-group">

                        @Html.TextBoxFor(model => model.Saida_1, new { @class = "form-control input-sm timeL", @placeholder = "--:--", id = string.Format("txtSaida1{0}", Model.Dia), @readonly="readonly" })
                        @Html.ValidationMessageFor(model => model.Saida_1)
                    </div>
                </div>
                <div class="col-md-2 col-xs-1">
                    <div class="checkbox" style="margin:0px;">
                        <label>
                            @Html.CheckBoxFor(model => model.Preassinaladas1Bool, new { id = string.Format("ckbPreassinaladas1{0}", Model.Dia) })
