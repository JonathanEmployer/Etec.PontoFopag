@model Modelo.HorarioDetalhe
@if (Model.Codigo == 0)
{
    <div class="row">
        <div class="col-md-1 col-xs-12"> </div>
        <div class="col-md-11 col-xs-12">
            <div class="col-md-1 col-xs-2" style="padding-left:0px; padding-right:0px">
                @Html.LabelFor(model => model.Entrada_1, new { @class = "control-label label-sm" })
            </div>
            <div class="col-md-3 col-xs-5" style="padding-left:0px; padding-right:0px">
