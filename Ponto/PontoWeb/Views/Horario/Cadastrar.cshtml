    function preencheIntervalo(lkp) {
        var pos = $(lkp).val().indexOf("|");
        if (pos > 0) {
            var horario = $(lkp).val();
            horario = horario.substr(pos + 1, horario.length);
            var horas = horario.split('-');
            var dia = lkp.replace("#lkpJornada", "");
            $("#txtEntrada1" + dia).val($.trim(horas[0]));
            $("#txtEntrada2" + dia).val($.trim(horas[2]));
            $("#txtEntrada3" + dia).val($.trim(horas[4]));
            $("#txtEntrada4" + dia).val($.trim(horas[6]));
            $("#txtSaida1" + dia).val($.trim(horas[1]));
            $("#txtSaida2" + dia).val($.trim(horas[3]));
            $("#txtSaida3" + dia).val($.trim(horas[5]));
            $("#txtSaida4" + dia).val($.trim(horas[7]));
            recalculaHorarioDia(dia);
        }
    }

    function recalculaHorarioDia(dia) {
        var entradas = [ConvertHoraMinuto($("#txtEntrada1" + dia).val()),
                        ConvertHoraMinuto($("#txtEntrada2" + dia).val()),
                        ConvertHoraMinuto($("#txtEntrada3" + dia).val()),
                        ConvertHoraMinuto($("#txtEntrada4" + dia).val())
        ];
        var saidas = [ConvertHoraMinuto($("#txtSaida1" + dia).val()),
                      ConvertHoraMinuto($("#txtSaida2" + dia).val()),
                      ConvertHoraMinuto($("#txtSaida3" + dia).val()),
                      ConvertHoraMinuto($("#txtSaida4" + dia).val())
        ];
        var inicioNoturno = ConvertHoraMinuto($("#InicioAdNoturno").val());
        var fimNoturno = ConvertHoraMinuto($("#FimAdNoturno").val());
        var tipoHorario = 1;
        if ($("#chkCargaMista").is(':checked')) {
            tipoHorario = 2;
        }
        var CalculaHorarioCafe = $("#DiasCafe" + dia).is(':checked');
        var PrimeiroPeridoCafe = $("#Habilitaperiodo01Bool").is(':checked');
        var SegundoPeriodoCafe = $("#Habilitaperiodo02Bool").is(':checked');
        var retorno = CalculaCargaHoraria(entradas, saidas, inicioNoturno, fimNoturno, tipoHorario, CalculaHorarioCafe, PrimeiroPeridoCafe, SegundoPeriodoCafe);
        $("#txtHorarioDiurno" + dia).val(ConvertMinutoHora(retorno.horasTrabalhadasDiurnas, 2));
        $("#txtHorarioNoturno" + dia).val(ConvertMinutoHora(retorno.horasTrabalhadasNoturnas, 2));
        $("#txtHorarioMisto" + dia).val(ConvertMinutoHora(retorno.horasTrabalhadasMista, 2));
        recalcularTotalizadorHoras();
        validaIntervaloAutomatico();
    }

    function recalcularTotalizadorHoras() {
        var total = 0;
        $(".totalHTrabDiurna").each(function () {
            total += ConvertHoraMinuto($(this).val());
        });
        $("#totDiurna").val(ConvertMinutoHora(total, 3));
        total = 0;
        $(".totalHTrabNoturna").each(function () {
            total += ConvertHoraMinuto($(this).val());
        });
        $("#totNoturna").val(ConvertMinutoHora(total, 3));
        total = 0;
        $(".totalHTrabMista").each(function () {
            total += ConvertHoraMinuto($(this).val());
        });
        $("#totMista").val(ConvertMinutoHora(total, 2));
    }

    function recalculaHorarios() {
        $(".lkpJornada").each(function () {
            var id = this.id;
            if (id.indexOf("lkpJornada") >= 0) {
                var dia = id.replace("lkpJornada", "");
                if (!isNaN(dia)) {
                    if (!$(this).prop('disabled')) {
                        recalculaHorarioDia(dia);
                    }
                    else {
                        $("#txtHorarioDiurno" + dia).val("");
                        $("#txtHorarioNoturno" + dia).val("");
                        $("#txtHorarioMisto" + dia).val("");
                        recalcularTotalizadorHoras();
                    }
                }
            }
        });
    }

