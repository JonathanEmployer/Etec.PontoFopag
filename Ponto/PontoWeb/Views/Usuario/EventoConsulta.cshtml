@model IEnumerable<Modelo.UsuarioPontoWeb>
@using System.Web.Optimization

@{
    Layout = "~/Views/Shared/_LayoutConsulta.cshtml";
}
<table id="tbSel" class="table table-striped table-bordered table-hover table-condensed nowrap" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Código</th>
            <th>Nome</th>
            <th>Login</th>
            <th>Grupo</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr id="@item.Id">
                <td>@item.Codigo</td>
                <td>@item.Nome</td>
                <td>@item.Login</td>
                <td>@item.GrupoUsuario</td>
                <td>@item.Email</td>
            </tr>
        }
</table>
