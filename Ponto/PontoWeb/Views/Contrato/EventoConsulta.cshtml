@model IEnumerable<Modelo.Contrato>
@using System.Web.Optimization

@{
    Layout = "~/Views/Shared/_LayoutConsulta.cshtml";
}
<table id="tbSel" class="table table-striped table-bordered table-hover table-condensed nowrap" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Código</th>
            <th>Empresa</th>
            <th>Código do Contrato</th>
            <th>Descrição</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr id="@item.Id">
                <td>@item.Codigo</td>
                <td>@item.NomeEmpresa</td>
                <td>@item.CodigoContrato</td>
                <td>@item.DescricaoContrato</td>
            </tr>
        }
</table>
           