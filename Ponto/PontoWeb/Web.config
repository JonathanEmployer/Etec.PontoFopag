    <section name="entityFramework"
      type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
      requirePermission="false"/>
    <section name="BufferLogConfig" type="Employer.PlataformaLog.Conf.BufferLogConfig, Employer.PlataformaLog"/>
    <section name="LogErrorConfig" type="Employer.PlataformaLog.Conf.LogErrorConfig, Employer.PlataformaLog"/>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <BufferLogConfig Tamanho="100" Timeout="00:00:30"/>
  <LogErrorConfig>
    <HttpLogErrorIgnore>
      <add statusCode="404"/>
      <add statusCode="403"/>
    </HttpLogErrorIgnore>
  </LogErrorConfig>
  <system.diagnostics>
    <sources>
      <source name="System.ServiceModel" switchValue="Error" propagateActivity="true">
        <listeners>
          <add name="LogErro" type="Employer.PlataformaLog.LogTrace,Employer.PlataformaLog"/>
        </listeners>
      </source>
      <source name="System.Web.Services.Asmx" switchValue="Error">
        <listeners>
          <add name="LogErro" type="Employer.PlataformaLog.LogTrace,Employer.PlataformaLog"/>
        </listeners>
      </source>
    </sources>
  </system.diagnostics>
  <connectionStrings configSource="Config\ConnectionStrings.dev.config"/>
  <appSettings file="Config\AppSettings.dev.config">
    <add key="ApiDB1" value="http://pontofopag.db1group.com/"/>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <add key="Employer.Componentes:HandlerCaptcha" value="/Componentes/HandlerCaptcha"/>
