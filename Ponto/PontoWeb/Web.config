    <add key="NewRelic.AppName" value="prd.pontofopag.com.br"/>
    <add key="appName" value="ServicoImportacaoRegistroPonto"/>
    <add key="ArquivosPontofopag" value="C:\Pontofopag\Arquivos"/>
    <add key="hangfireQueues" value=""/>
  </appSettings>
  <location path="Componentes/HandlerCaptcha">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="hangfire">
    <system.web>
      <authorization>
        <deny users="?"/>
      </authorization>
    </system.web>
  </location>
